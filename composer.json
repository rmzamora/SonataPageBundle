{
    "name": "rmzamora/page-bundle",
    "type": "symfony-bundle",
    "description": "Symfony SonataPageBundle rmzamora forked",
    "keywords": ["page", "sonata", "cms", "rmzamora forked"],
    "homepage": "http://sonata-project.org/bundles/page",
    "license": "MIT",
    "authors": [
        {
            "name": "Mell M. Zamora",
            "email": "me@mellzamora.com",
            "homepage": "http://mellzamora.com"
        },
        {
            "name": "Thomas Rabaix",
            "email": "thomas.rabaix@sonata-project.org",
            "homepage": "http://sonata-project.org"
        },
        {
            "name": "Sonata Community",
            "homepage": "https://github.com/sonata-project/SonataPageBundle/contributors"
        }
    ],
    "require": {
        "sonata-project/admin-bundle": "2.1.*@dev",
        "sonata-project/seo-bundle": "1.*",
        "sonata-project/doctrine-extensions" : "1.*",
        "sonata-project/cache-bundle": "2.1.*",
        "symfony-cmf/routing-extra-bundle": "1.0.*@dev",
        "symfony-cmf/routing": "1.0.*@dev"
    },
    "require-dev": {
        "sonata-project/doctrine-orm-admin-bundle": "2.1.*@dev"
    },
    "suggest": {
        "sonata-project/doctrine-orm-admin-bundle": "2.1.*@dev",
        "sonata-project/notification-bundle": "2.1.*"
    },
    "autoload": {
        "psr-0": { "Sonata\\PageBundle": "" }
    },
    "target-dir": "Sonata/PageBundle",
    "extra": {
        "branch-alias": {
            "dev-2.0": "2.0.x-dev",
            "dev-2.1": "2.1.x-dev",
            "dev-master": "2.2.x-dev"
        }
    }
}
