{
    "name": "rmzamora/page-bundle",
    "type": "symfony-bundle",
    "description": "Symfony SonataPageBundle rmzamora forked",
    "keywords": ["page", "sonata", "cms", "rmzamora forked"],
    "homepage": "http://sonata-project.org/bundles/page",
    "license": "MIT",
    "authors": [
        {
            "name": "Mell M. Zamora",
            "email": "me@mellzamora.com",
            "homepage": "http://mellzamora.com"
        },
        {
            "name": "Thomas Rabaix",
            "email": "thomas.rabaix@sonata-project.org",
            "homepage": "http://sonata-project.org"
        },
        {
            "name": "Sonata Community",
            "homepage": "https://github.com/sonata-project/SonataPageBundle/contributors"
        }
    ],
    "require": {
        "symfony/symfony": "~2.4",
        "rmzamora/admin-bundle": "~2.3",
        "rmzamora/seo-bundle": "~1.1",
        "sonata-project/doctrine-extensions" : "~1.0",        
        "rmzamora/cache-bundle": "~2.2",
        "rmzamora/cache": "~1.0@dev",
        "sonata-project/core-bundle": "~2.2",
        "symfony-cmf/routing-bundle": "~1.1",
        "rmzamora/notification-bundle": "~2.3"
    },
    "require-dev": {
     	"rmzamora/rest-bundle": "~1.3",
        "rmzamora/doctrine-orm-admin-bundle": "~2.3"
    },
    "suggest": {
        "rmzamora/doctrine-orm-admin-bundle": "~2.3"
    },    
    "autoload": {
        "psr-0": { "Sonata\\PageBundle": "" }
    },
    "target-dir": "Sonata/PageBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "2.3.x-dev"
        }
    }
}
