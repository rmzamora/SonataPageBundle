{
    "name": "rmzamora/page-bundle",
    "type": "symfony-bundle",
    "description": "Symfony SonataPageBundle rmzamora forked",
    "keywords": ["page", "sonata", "cms", "rmzamora forked"],
    "homepage": "https://sonata-project.org/bundles/page",
    "license": "MIT",
    "authors": [
        {
            "name": "Mell M. Zamora",
            "email": "me@mellzamora.com",
            "homepage": "http://mellzamora.com"
        },
        {
            "name": "Thomas Rabaix",
            "email": "thomas.rabaix@sonata-project.org",
            "homepage": "https://sonata-project.org"
        },
        {
            "name": "Sonata Community",
            "homepage": "https://github.com/sonata-project/SonataPageBundle/contributors"
        }
    ],
    "require": {
        "symfony/symfony": "~2.4",
        "rmzamora/admin-bundle": "2.4.*",
        "rmzamora/seo-bundle": "2.0.*",
        "sonata-project/doctrine-extensions" : "~1.0",        
        "rmzamora/cache-bundle": "~2.2",
        "rmzamora/cache": "~1.0@dev",
        "sonata-project/core-bundle": "~2.3,>=2.3.1",
        "sonata-project/datagrid-bundle": "~2.2@dev",
        "symfony-cmf/routing-bundle": "~1.1",
        "rmzamora/notification-bundle": "2.4.*",
        "friendsofsymfony/rest-bundle": "~1.1",
        "jms/serializer-bundle": "~0.13",
        "nelmio/api-doc-bundle": "~2.4"        
    },
    "require-dev": {
    	"rmzamora/admin-bundle": "2.4.*",
        "rmzamora/doctrine-orm-admin-bundle": "2.4.*",
        "sonata-project/core-bundle": "~2.3,>=2.3.1",
        "symfony/phpunit-bridge": "~2.7|~3.0"
    },
    "suggest": {
        "rmzamora/doctrine-orm-admin-bundle": "2.4.*"
    },    
    "autoload": {
        "psr-4": { "Sonata\\PageBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-2.4": "2.4.x-dev"
        }
    }
}
